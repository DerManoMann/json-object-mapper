{
    "name": "radebatz/object-mapper",
    "description": "(Json) Object mapper.",
    "type": "library",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\ObjectMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radebatz\\ObjectMapper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --allow-risky=yes",
        "lint": "@cs --dry-run",
        "test": [
          "phpunit",
          "@lint"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "psr/log": "^1.0",
        "radebatz/property-info-extras": "^1.1",
        "symfony/property-access": "^4.0|^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "monolog/monolog": "^1.23|^2.0",
        "netresearch/jsonmapper": "^3.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/php-code-coverage": "^6.1|^7.0.5",
        "phpunit/phpunit": "^8.0"
    }
}
