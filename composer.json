{
    "name": "radebatz/object-mapper",
    "description": "(Json) Object mapper.",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\ObjectMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radebatz\\ObjectMapper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "fix-cs": [
            "vendor/bin/php-cs-fixer fix src",
            "vendor/bin/php-cs-fixer fix tests"
        ]
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "phpdocumentor/reflection-docblock": "^4.3",
        "psr/log": "^1.0",
        "symfony/property-access": "^3.4|^4.0",
        "symfony/property-info": "^4.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "monolog/monolog": "^1.23",
        "netresearch/jsonmapper": "^1.4",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/php-code-coverage": "^6.1|^7.0.5",
        "phpunit/phpunit": "^7.0"
    }
}
